  "dependencies": {
    "strict-event-emitter-types": "^2.0.0",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/ws": "^7.2.6",
    "jest": "^26.6.3",
